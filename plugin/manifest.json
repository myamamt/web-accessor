{
  "name": "WebAccessor",
  "version": "1.0",
  "description": "リスト上の様々なページにアクセスするプラグイン",
  "browser_action": {
    "default_title": "WebAccessor",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "background": {
    "scripts": ["url.js", "background.js"],
    "persistent": true
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "run_at": "document_end",
    "js": [ "content_script.js"],
    "all_frames": false
  }],
  "permissions": [
    "debugger",
    "tabs"
  ]
}
